<?xml version="1.0" encoding="utf-8"?>
<ScrollView
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

        <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="NORMAL_GET_OBJ"
                android:id="@+id/btn_normal_get_obj"/>

        <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="NORMAL_GET_ARRAY"
                android:id="@+id/btn_normal_get_array"/>

        <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="NORMAL_POST_OBJ"
                android:id="@+id/btn_normal_post_obj"/>

        <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="NORMAL_POST_ARRAY"
                android:id="@+id/btn_normal_post_array"/>

        <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="CANCEL_NORMAL_GET_OBJ"
                android:id="@+id/btn_normal_get_obj_cancel"/>

        <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="HIGH_GET_OBJ"
                android:id="@+id/btn_high_get_obj"/>

        <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="NORMAL_GET_SYNC_RETURN"
                android:id="@+id/btn_normal_get_obj_sync_return"/>

        <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="NORMAL_GET_SYNC_CALLBACK"
                android:id="@+id/btn_normal_get_obj_sync_callback"/>

        <TextView
                android:id="@+id/tv_result"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
    </LinearLayout>
</ScrollView>